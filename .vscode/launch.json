{
    "version": "2.0.0",
    "configurations": [
        {
            "name": "Debug ImageCompressCpp",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build-linux-debug/ImageCompress",
            "args": [
                "--outdir", "/home/chris/Pictures/website/sm",
                "--imgdir", "/home/chris/Pictures/website/",
                "--width", "30",
                "--quality", "30"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "preLaunchTask": "Build Linux Debug",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}